<?xml version="1.0"?>
<info>
	<id>misp_bot</id>
	<name>MISPBot</name>
	<summary>Nextcloud MISPBot</summary>
	<description>
	<![CDATA[A NextCloud MISP Bot for easy importing of IoC. The Bot makes sure that only public IPv4 can be submitted.]]>
	</description>
	<version>1.0.0</version>
	<licence>MIT</licence>
	<author mail="patrice@kasts.ch" homepage="https://github.com/PatriceKast">Patrice Kast</author>
	<namespace>MISPBot</namespace>
	<category>tools</category>
	<website>https://github.com/PatriceKast/NextCloud-Bot-MISP-IoC-Importer</website>
	<bugs>https://github.com/PatriceKast/NextCloud-Bot-MISP-IoC-Importer/issues</bugs>
	<repository type="git">https://github.com/PatriceKast/NextCloud-Bot-MISP-IoC-Importer</repository>
	<dependencies>
		<nextcloud min-version="28" max-version="30"/>
	</dependencies>
	<external-app>
		<docker-install>
			<registry>ghcr.io</registry>
			<image>kastpatrice/misp_bot</image>
			<image-tag>latest</image-tag>
		</docker-install>
		<scopes>
			<value>TALK</value>
			<value>misp_bot</value>
		</scopes>
	</external-app>
</info>
